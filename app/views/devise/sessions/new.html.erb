
        <div class="col-sm" style="text-align:center;margin-bottom:20px;padding-top: 50px;">
          <%= devise_error_messages! %>
            <div class="" style="display:block;width:100%;text-align:left;">
              <h1 style="font-weight: 900;">Log in to Unirocket</h1>

            </div>

            <div class="" style="display:block;width:100%;text-align:left;">
              <h5 style="font-weight: 700;">DeFi tokens paradise.</h5>

            </div>

              <br>

              <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
                <div class="field">
                  <%= f.label :email, class: "pull-left"  %><br />
                  <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "form-control" %>
                </div>
                <br>
                <div class="field">
                  <%= f.label :password, class: "pull-left" %><br />
                  <%= f.password_field :password, autocomplete: "current-password", class: "form-control" %>
                </div>

                <br>

                <% if devise_mapping.rememberable? %>
                  <div class="field">
                    <%= f.check_box :remember_me %>
                    <%= f.label :remember_me %>
                  </div>
                <% end %>

                <div class="actions">
                  <%= f.submit "Log in", class: "btn btn-kuulutused", style: "width:100px;font-weight:700;" %>
                        <%= link_to "Sign up", new_registration_path(resource_name), class: "btn btn-kuulutused", style: "width:100px;font-weight:700;" %>
                </div>
              <% end %>

              <br>
                <%= link_to "Forgot your password?", new_password_path(resource_name) %><br />
         </div>



